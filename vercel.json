{
  "version": 2,
  "builds": [
    {
      "src": "api/server.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/serverTwo.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/server1",
      "dest": "api/server.py"
    },
    {
      "src": "/server2",
      "dest": "api/serverTwo.py"
    }
  ]
}
